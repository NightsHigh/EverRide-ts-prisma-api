generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
}

model User {
  id Int @id @default(autoincrement())
  name String
  email String @unique
  password String

  @@map("user")
}

model Category {
  id Int @id @default(autoincrement())
  name String @unique

  @@map("categories")
}

model Brand {
  id Int @id @default(autoincrement())
  name String @unique

  @@map("brands")
}

model Car {
  id Int @id @default(autoincrement())
  category String
  brand String
  model String
  year Int
  price String
  fueltype String

  @@map("cars")
}